{
  "name": "smiler-backend",
  "version": "1.0.0",
  "description": "Backend module of Smiler project",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=v20.16.0"
  },
  "scripts": {
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --maxWorkers 4",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "gen-doc": "npx swagger-jsdoc -d src/swagger/swaggerDef.js -o \"src/swagger/swagger.json\"",
    "postinstall": "npm run gen-doc",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "prettier": "prettier . --write"
  },
  "keywords": [
    "express",
    "blog",
    "api",
    "rest"
  ],
  "author": "Darkzarich",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^2.1.1",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.0.0",
    "nodemon": "^3.1.2",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "connect-mongo": "^3.0.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^3.6.0",
    "dotenv": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.18.2",
    "express-request-id": "^1.4.1",
    "express-session": "^1.17.1",
    "helmet": "^7.1.0",
    "mongoose": "^5.7.5",
    "mongoose-autopopulate": "^0.9.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "prettier": "^3.3.2",
    "sanitize-html": "^2.13.0",
    "sharp": "^0.33.4",
    "slug": "^1.1.0",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.0.7",
    "winston": "^3.2.1"
  }
}
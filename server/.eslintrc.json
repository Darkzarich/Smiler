{
  "root": false,
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:node/recommended",
    "plugin:security/recommended-legacy",
    "prettier"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "consistent-return": "off",
    "no-underscore-dangle": [
      "error",
      { "allow": ["_id", "_handleFile", "_removeFile"] }
    ]
  }
}

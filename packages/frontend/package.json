{
  "name": "@smiler/frontend",
  "description": "Frontend module of Smiler project",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=v20.16.0",
    "pnpm": ">=8.6.0"
  },
  "files": [
    "dist",
    "change-api-url.sh",
    "nginx.conf"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test:e2e:ci": "pnpm exec playwright test --reporter=line",
    "test:e2e": "pnpm exec playwright test",
    "test:e2e:ui": "pnpm exec playwright test --ui",
    "lint:css": "pnpm exec stylelint '**/*.{css,scss,vue}'",
    "lint:ts": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:ts:fix": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix .",
    "lint:types": "pnpm exec vue-tsc --noEmit",
    "prettier": "prettier \"**/*.{js,ts,scss,css,html,json,vue}\" --write"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "click-outside-vue3": "4.0.1",
    "core-js": "^3.44.0",
    "date-fns": "^3.6.0",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.3",
    "typescript": "^5.9.2",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.54.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-playwright": "^1.8.3",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "postcss-html": "^1.8.0",
    "prettier": "3.2.5",
    "sass": "^1.89.2",
    "stylelint": "^16.23.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-two-dash-bem": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.12.1",
    "vite": "^6.3.5",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.5"
  }
}

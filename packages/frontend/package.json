{
  "name": "@smiler/frontend",
  "description": "Frontend module of Smiler project",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=v20.16.0",
    "pnpm": ">=8.6.0"
  },
  "files": [
    "dist",
    "change-api-url.sh",
    "nginx.conf"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test:e2e:ci": "pnpm exec playwright test --reporter=line",
    "test:e2e": "pnpm exec playwright test",
    "test:e2e:ui": "pnpm exec playwright test --ui",
    "lint:css": "pnpm exec stylelint '**/*.{css,scss,vue}'",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ext .js,.vue --ignore-path .gitignore --fix .",
    "prettier": "prettier \"src/**/*.{js,scss,css,html,json,vue}\" --write"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "core-js": "^3.27.2",
    "date-fns": "^3.6.0",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "v-click-outside": "^2.1.4",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.0.1",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-airbnb": "^7.0.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-playwright": "^1.5.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^10.1.0",
    "postcss-html": "^1.6.0",
    "prettier": "3.2.5",
    "sass": "^1.87.0",
    "stylelint": "^16.19.1",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-config-two-dash-bem": "^1.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.11.1",
    "vite": "^6.3.4"
  }
}
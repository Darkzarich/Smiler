{
  "name": "@smiler/backend",
  "version": "1.0.0",
  "description": "Backend module of Smiler project",
  "main": "index.js",
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=v20.16.0",
    "pnpm": ">=8.6.0"
  },
  "files": [
    "dist/src",
    "dist/index.js",
    "tsconfig.json"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --runInBand --forceExit --detectOpenHandles",
    "build": "tsc && tsc-alias -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "ts-node index.ts",
    "dev:watch": "nodemon --exec ts-node index.ts",
    "dev:debug": "node --inspect -r ts-node/register index.ts",
    "lint:code": "eslint --ext .js --ext .ts --ignore-path .gitignore .",
    "lint:types": "tsc --noEmit",
    "prettier": "prettier . --write"
  },
  "keywords": [
    "express",
    "typescript",
    "blog",
    "api",
    "rest",
    "ts-node",
    "node",
    "nodemon",
    "swc"
